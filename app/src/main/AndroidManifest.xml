<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/><!-- Permiso para poder usar el JDBC-->
    <uses-permission android:name="android.permission.WAKE_LOCK" />


    <application
        android:usesCleartextTraffic="true"

        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Testeo"
        tools:targetApi="30">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".ActivityLogin"></activity>
        <activity android:name=".ActivityRegister"></activity>
        <activity android:name=".ActivityRegister2"></activity>
        <activity android:name=".ActivityUI"></activity>
        <activity android:name=".ActivityAddCar"></activity>
        <activity android:name=".ActivityMyCars"></activity>
        <activity android:name=".ActivityAddComponent"></activity>
        <activity android:name=".ActivityConfig"></activity>
    </application>

</manifest>